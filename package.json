{
  "name": "spotify-graphitejs",
  "version": "0.0.1",
  "private": false,
  "description": "website",
  "repository": "",
  "author": "Walter Zalazar",
  "scripts": {
    "build": "next build",
    "build:testing": " NODE_ENV=testing next build",
    "start": "next start",
    "dev": "next",
    "start:server": "node index.js",
    "start:all": "npm run dev & npm run start:server",
    "start:dev": "npm run start:all & json-proxy -p 8080 -f '/graphql=http://localhost:4000' -f '/=http://localhost:3000' .",
    "now-build": "next build",
    "lint": "eslint -f table *.js || exit 1",
    "lint:fix": "eslint --fix *.js || exit 1",
    "uml:architecture": "puml generate ./.uml/architecture.puml -o ./.uml/architecture.png",
    "semantic-release": "semantic-release",
    "test:unit": "jest",
    "test:functional": "NODE_ENV=testing testcafe ${RUNNER:-chrome:headless} tests/functional/features --app 'npm run start'",
    "coverage:unit": "jest --coverage"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@graphite/server": "0.10.14",
    "annyang": "2.6.1",
    "dataloader": "1.4.0",
    "es6-promise": "4.2.8",
    "esm": "3.2.25",
    "ip": "1.1.5",
    "load-script": "1.0.0",
    "lodash": "4.17.19",
    "micro": "9.3.4",
    "next": "8.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-fastclick": "3.0.2",
    "react-progressbar.js": "0.2.0",
    "react-redux": "6.0.1",
    "redux": "4.0.5",
    "redux-combine-actions": "0.1.2",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.2",
    "serverless-http": "1.10.1",
    "spotify-web-api-node": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.0",
    "@babel/helper-plugin-utils": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.10.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.10.0",
    "@babel/plugin-proposal-optional-chaining": "7.10.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.8.3",
    "@babel/register": "7.9.0",
    "@babel/runtime": "7.10.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "chai": "4.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "jest": "24.9.0",
    "json-proxy": "0.9.3",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "0.19.0",
    "next-images": "1.4.1",
    "node-plantuml": "0.5.0",
    "node-sass": "4.13.0",
    "postcss": "7.0.32",
    "postcss-easy-import": "3.0.0",
    "postcss-js": "2.0.3",
    "postcss-loader": "4.0.0",
    "raw-loader": "3.1.0",
    "react-addons": "0.9.1-deprecated",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.13.1",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.4",
    "redux-test-utils": "0.3.0",
    "regenerator-runtime": "0.13.7",
    "sass-loader": "9.0.2",
    "semantic-release": "17.1.1",
    "sinon": "9.0.1",
    "styled-jsx-plugin-sass": "1.0.0",
    "testcafe": "0.23.3"
  },
  "jest": {
    "coverageDirectory": ".coverage",
    "verbose": true,
    "globals": {
      "window": {}
    },
    "moduleNameMapper": {
      ".+\\.(css|less|scss)$": "<rootDir>/.mocks/jest/CSSStub.js"
    },
    "collectCoverageFrom": [
      "components/**/*.js",
      "componentsBusiness/**/*.js",
      "containers/**/*.js",
      "lib/**/*.js",
      "pages/**/*.js",
      "sever/**/*.js"
    ]
  }
}

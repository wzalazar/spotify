{
  "name": "spotify-graphitejs",
  "version": "0.0.1",
  "private": false,
  "description": "website",
  "repository": "",
  "author": "Walter Zalazar",
  "scripts": {
    "lint": "eslint -f table ./components ./config ./containers ./lib ./pages ./server || exit 1",
    "lint:fix": "eslint --fix ./components ./config ./containers ./lib ./pages ./server || exit 1",
    "start": "NODE_ENV=production node -r esm ./index.js",
    "start:dev": "node -r esm ./index.js",
    "build": "next build",
    "uml:architecture": "puml generate ./.uml/architecture.puml -o ./.uml/architecture.png",
    "semantic-release": "semantic-release",
    "test:unit": "jest",
    "test:functional": "NODE_ENV=production testcafe ${RUNNER:-chrome:headless} tests/functional/features",
    "coverage:unit": "jest --coverage"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@graphite/server": "0.8.7",
    "annyang": "2.6.0",
    "dataloader": "1.4.0",
    "es6-promise": "4.2.5",
    "esm": "3.2.7",
    "ip": "1.1.5",
    "load-script": "1.0.0",
    "lodash": "4.17.4",
    "micro": "9.3.3",
    "next": "8.0.3",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-fastclick": "3.0.2",
    "react-progressbar.js": "0.2.0",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-combine-actions": "0.1.2",
    "redux-observable": "1.0.0",
    "rxjs": "6.4.0",
    "spotify-web-api-node": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/helper-plugin-utils": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.3.4",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.3.1",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "5.2.9",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "autoprefixer": "9.4.7",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "chai": "4.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.10.0",
    "eslint": "5.13.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.1.0",
    "json-proxy": "0.9.3",
    "next-compose-plugins": "2.1.1",
    "next-fonts": "0.16.0",
    "next-images": "1.0.4",
    "node-plantuml": "0.5.0",
    "node-sass": "4.5.3",
    "postcss": "7.0.7",
    "postcss-easy-import": "3.0.0",
    "postcss-js": "2.0.0",
    "postcss-loader": "3.0.0",
    "raw-loader": "1.0.0",
    "react-addons": "0.9.1-deprecated",
    "react-addons-test-utils": "15.5.1",
    "react-test-renderer": "16.8.3",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-test-utils": "0.3.0",
    "regenerator-runtime": "0.13.1",
    "sass-loader": "7.1.0",
    "semantic-release": "15.13.3",
    "sinon": "7.2.4",
    "styled-jsx-plugin-sass": "1.0.0",
    "testcafe": "0.23.3"
  },
  "jest": {
    "coverageDirectory": ".coverage",
    "verbose": true,
    "globals": {
      "window": {}
    },
    "moduleNameMapper": {
      ".+\\.(css|less|scss)$": "<rootDir>/.mocks/jest/CSSStub.js"
    },
    "collectCoverageFrom": [
      "components/**/*.js",
      "componentsBusiness/**/*.js",
      "containers/**/*.js",
      "lib/**/*.js",
      "pages/**/*.js",
      "sever/**/*.js"
    ]
  }
}
